{
  "name": "dns-client-app",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build && wrangler dev",
    "build": "npm run build:client && npm run build:worker",
    "build:client": "vite build",
    "build:worker": "tsc -p worker/tsconfig.json && esbuild worker/worker.ts --bundle --format=esm --platform=neutral --target=es2022 --outfile=dist/worker.js --tsconfig=tsconfig.json --external:net --external:dgram --external:tls --external:crypto --external:fs --external:path --external:url --external:stream --external:http --external:https --external:node:*",
    "start": "wrangler dev",
    "check": "tsc"
  },
  "dependencies": {
    "@aoxborrow/dns-client": "^0.1.7",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@tailwindcss/typography": "^0.5.0-alpha.3",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@types/node": "20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.25.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.6.3",
    "vite": "^5.4.20",
    "wrangler": "^4.47.0"
  }
}
